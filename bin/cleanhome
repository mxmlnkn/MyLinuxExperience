#!/usr/bin/env bash

# deletes some unneeded caches in home folder

rm -rf $HOME/.debug   # binaries from coredumps, I think
rm -rf $HOME/.local/share/cargo/registry/

# Folder thumbnails and such
rm -rf $HOME/.cache/thumbnails
rm -rf $HOME/.thumbnails
rm -rf $HOME/.debug
rm -f $HOME/.xnviewmmp/Thumb.db
rm -f $HOME/.xnviewmmp/XnView.db
rm -f $HOME/.ccache/ccache.log
